<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <diskStore path="java.io.tmpdir"/> <!--Este es un directorio temporal de Java-->
    <defaultCache maxElementsInMemory="100" eternal="false" timeToIdleSeconds="5" timeToLiveSeconds="10" 
        overFlowToDisk="true"/>
    <!-- MaxElementsInMemory: Cantidad m치xima de elementos cacheados -->
    <!-- Eterno: Si vivir치 para siempre o no -->
    <!-- TimeToIdleSeconds: Cantidad de tiempo hasta ser marcado como idle y removido si no se usa -->
    <!-- TImeToLiveSeconds: Cantidad de tiempo tras los cuales ser치 removido, sin importar si es accedido o no -->
    <!-- OverFlowToDisk: Si se escribir치 en el disco terminando -->
</ehcache>

